generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company_data {
  id                String   @id @default(uuid())
  organization_name String
  industry          String
  city              String?
  created_at        DateTime @default(now())
}

model Company_emails {
  id                String   @id @default(uuid())
  organization_name String
  first_name        String
  last_name         String?
  email             String   @unique
  title             String?
  linkedin_url      String?
  created_at        DateTime @default(now())
}
